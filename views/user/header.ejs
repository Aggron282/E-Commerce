<div class="navbar">

    <div class="row return_row no_padding" >

      <div class="col-1 no_margin no-padding ">

        <a href = "/">
          <img src = <%= `./${root}/images/e_logo.png`%> class=" margin-top-7_5 logo" alt = "logo" class="logo"/>
        </a>

      </div>

      <div class="col-2 no_margin  no-padding margin-top-2_5">
          <p class="return_to margin-left-5 padding-left-10">
            <strong>Delivering to</strong>
            <br />
            Scottdale Arizona 852....
            <br />
            <span>
              Update Location
            </span>
        </p>
      </div>

      <div class="col-3  no-padding margin-top-2_5">

          <div>

              <span class="item_search_bar" class="form-control">

                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
                    <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001q.044.06.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1 1 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0"/>
                  </svg>

              </span>

              <input class="form-control no-border search_all "   placeholder = "Search for Item">

          </div>

      </div>

      <% if (! isAuthenticated){ %>

      <div class="col-2 no_margin no-padding margin-top-2_5"style="margin-left:5%">

          <p class="return_to" style="font-size:15px">
            Hello! Sign in to
            <br/>

            <span class="bold"style="font-size:15px;">
              <a href="/login">Customer</a> |  Admin
            </span>

          </p>

      </div>



      <% }else{ %>

        <div class="col-1 order_col margin-left-5" style="margin-left:7.5%">
            <p class="header_button">Your Orders </p>
        </div>

        <div class="col-1 order_col margin-left-5" style="margin-left:1%" >

          <a href = "/logout" >

            <div class=" header_button text-center">
              Logout
            </div>

          </a>

        </div>

      <%}%>




        <% if(isAuthenticated){ %>
          <div class="col-1 return_col relative no-padding margin-top-2_5"style="margin-left:7.5%" >

          <a href="/cart">

            <img src=<%= `./${root}/images/cart.png` %> class="return_cart" />

            <div class="bubble_cart_return">

              <% if(cart){ %>

                  <% if(cart.items.length > 0) { %>
                    <p id="cart_number" > <%= cart.items.length %> </p>
                 <% }else{ %>
                   <p id="cart_number">0</p>
                  <%}%>
              <% } %>

            </div>

          </a>

            <%}else{%>
              <div class="col-2 relative no-padding margin-top-2_5" style="margin-left:5%" >

              <a href = "/create_account">
                <button class=" header_button" > Create Account </button>
              </a>

            <%}%>

      </div>

   </div>

</div>

<div class = "sub_banner">
    <% console.log(catagories) %>
    <% if(catagories){ %>

      <% if(catagories.length > 0 ){ %>

        <% for(var i = 0; i < catagories.length; i++){ %>

              <p class="sub_catagory"><%= catagories[i].catagory %>
                <span class="sub_underline"></span>
              </p>
        <% } %>

        <% } %>

    <% } %>

 </div>
